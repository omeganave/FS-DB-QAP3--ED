<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Post List</title>
    </head>
    <body>
        <h1>Post List</h1>
        <ul>
            <% posts.forEach(post => { %>
                <li>
                    <a href="/posts/<%= post.id %>"><%= post.title %></a>
                </li>
            <% }) %>
        </ul>
    </body>
</html>